<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.next.volley" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.RequestQueueSetUpDemo" />
        <activity
            android:name=".activity.SingleInstanceActivity"
            android:label="@string/title_activity_single_instance" >
        </activity>
        <activity
            android:name=".activity.StandardRequestActivity"
            android:label="@string/title_activity_standard_request" >
        </activity>
        <activity
            android:name=".activity.JSONRequestActivity"
            android:label="@string/title_activity_jsonrequest" >
        </activity>
        <activity
            android:name=".activity.FastJsonRequestActivity"
            android:label="@string/title_activity_fast_json_request" >
        </activity>
    </application>

</manifest>
